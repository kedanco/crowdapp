<script type="text/javascript">


function initMap() {
        // Create the map.
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 12,
          center: {lat: 1.350452, lng:103.820581},
          mapTypeId: 'roadmap'
        });

      var transitLayer = new google.maps.TransitLayer();
      transitLayer.setMap(map);
}


